{
  "name": "esketch",
  "version": "0.0.0",
  "author": "Claude SIMON (http://q37.info/contact/)",
  "description": "XML parser and preprocessor.",
  "main": "esketch.js",
  "engines": {
    "node": ">=4"
  },
  "binary": {
    "module_name": "esketchnjs",
    "module_path": "./lib/{platform}-{arch}/",
    "remote_path": "./download/computing/npm/{module_name}/v{version}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://q37.info"
  },
  "dependencies": {
    "node-pre-gyp": "0.6.x"
  },
  "devDependencies": {
    "aws-sdk": "2.x"
  },
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epeios-q37/esketch-node.git"
  },
  "keywords": [
    "addon",
    "XML",
    "parser",
    "preprocessor",
    "xpp",
    "q37",
    "epeios"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/epeios-q37/esketch-node/issues"
  },
  "homepage": "https://github.com/epeios-q37/esketch-node#readme"
}
